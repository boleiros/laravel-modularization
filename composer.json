{
    "name": "boleiros/laravel-modularization",
    "description": "Commands to automate the creation of modules in laravel",
    "authors": [
        {
            "name": "Elton Antunes",
            "email": "eltonantunes@hotmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
          "Boleiros\\Modularization\\": "src/Modularization/"
        }
      },
    "require-dev": {
        "orchestra/testbench": "~3.5"
    },
    "config":{
    "preferred-install": "dist"
    },
    "minimum-stability" : "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Modularization\\Bake\\Providers\\DefenderServiceProvider"
            ]
        }
    }
}
